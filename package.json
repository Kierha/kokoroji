  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "dotenv": "^17.2.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.31.0",
    "eslint-config-expo": "^9.2.0",
    "jest": "^29.7.0",
    "typescript": "~5.8.3"
